{
  "name": "sortr",
  "version": "0.2.1",
  "description": "CLI tool that organizes a directory of photos by sorting them into subdirectories named after date the photo was taken",
  "main": "./bin/sortr.js",
  "bin": {
    "sortr": "./bin/sortr.js"
  },
  "keywords": [
    "organize",
    "photos",
    "images",
    "directories",
    "date"
  ],
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "bluebird": "2.2.x",
    "chalk": "0.5.x",
    "exifdata": "0.2.x",
    "fs-extra": "0.10.x",
    "is-jpg": "0.1.x",
    "lodash": "2.4.x",
    "moment": "2.8.x",
    "optimist": "0.6.x",
    "read-chunk": "0.1.x"
  },
  "devDependencies": {
    "coffee-script": "*",
    "gulp": "*",
    "gulp-clean": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-template": "*",
    "gulp-util": "*"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/sortr"
  },
