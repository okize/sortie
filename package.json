    "mkdirp": "*",
    "mmmagic": "*",
    "moment": "*",
    "optimist": "*"
  },
  "devDependencies": {
    "coffee-script": "*",
    "gulp": "*",
    "gulp-clean": "*",
    "gulp-coffee": "*",
    "gulp-coffeelint": "*",
    "gulp-template": "*",
    "gulp-util": "*"
  },
